{
    "expo": {
        "name": "PIEC",
        "slug": "image-gallery-client",
        "privacy": "public",
        "sdkVersion": "35.0.0",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#eeeeee"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.github.f4irline.imageGallery"
        },
        "android": {
            "package": "com.github.f4irline.imageGallery"
        }
    }
}
